<div class="bank_account">
  <div class="control-group">
    <%= f.label :bank, :class => 'control-label' %>
    <div class="controls">
      <div class="input-prepend">
        <span class="add-on"><i class="icon-phone"></i></span><%= f.text_field :bank, :class => 'text_field' %>
      </div>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :bank_account, :class => 'control-label' %>
    <div class="controls">
      <div class="input-prepend">
        <span class="add-on"><i class="">C</i></span><%= f.text_field :bank_account, :class => 'text_field' %>
      </div>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :sinpe, :class => 'control-label' %>
    <div class="controls">
      <div class="input-prepend">
        <span class="add-on"><i class="">S</i></span><%= f.text_field :sinpe, :class => 'text_field' %>
      </div>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :account_title, :class => 'control-label' %>
    
    <div class="controls">
      <div class="input-prepend">
        <span class="add-on"><i class="icon-user"></i></span><%= f.text_field :account_title, :class => 'text_field' %>
      </div>
      <%= f.hidden_field :_destroy %>
      <%= link_to I18n.t(".helpers.links.destroy"), '#', class: "remove_bank_account btn btn-mini btn-danger" %>
    </div>
  </div>
<hr />
</div>
