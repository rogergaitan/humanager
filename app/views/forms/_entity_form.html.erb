<div class="form-group">
	<%= f.label :name, :class => 'col-sm-2 control-label' %>
	<div class="col-sm-3">
		<div class="input-group">
			<span class="input-group-addon"><i class="fa fa-user"></i></span>
			<%= f.text_field :name, :class => 'form-control', :readonly => 'true' %>
		</div>
	</div>
</div>

<div class="form-group">
	<%= f.label :surname, :class => 'col-sm-2 control-label' %>
	<div class="col-sm-3">
		<div class="input-group">
			<span class="input-group-addon">A</span>
			<%= f.text_field :surname, :class => 'form-control', :readonly => 'true' %>
		</div>
	</div>
</div>

<div class="form-group">
	<%= f.label :entityid, :class => 'col-sm-2 control-label' %>
	<div class="col-sm-3">
		<div class="input-group">
			<span class="input-group-addon">C</span>
			<%= f.text_field :entityid, :class => 'form-control', :readonly => 'true' %>
		</div>
	</div>
</div>

<div class="form-group">
  <%= f.label :typeid, :class => 'col-sm-2 control-label' %>
  <div class="col-sm-3">
		<div class="input-group">
			<span class="input-group-addon">T</span>
			<%= f.enum_select(:typeid, {:include_blank => false}, {:class => 'form-control'} ) %>
		</div>
  </div>
</div>
