<div class="form-group">
  <%= f.label :country, :class => 'col-sm-2 control-label' %>
  <div class="col-sm-3">
    <%= f.text_field(:country, html_options = {:class => 'form-control', :disabled => true}) %>
  </div>
</div>

<div class="form-group">
  <%= f.label :department, :class => 'col-sm-2 control-label' %>
  <div class="col-sm-3">
    <%= f.text_field(:department, html_options = {:class => 'form-control', :disabled => true}) %>
  </div>
</div>

<div class="form-group">
  <%= f.label :municipality, :class => 'col-sm-2 control-label' %>
  <div class="col-sm-3">
    <%= f.text_field(:municipality, html_options = {:class => 'form-control', :disabled => true}) %>
  </div>
</div>

<div class="form-group">
	<%= f.label :address, :class => 'col-sm-2 control-label' %>
	<div class="col-sm-3">
		<%= f.text_area :address, :size => "5x5", :class => 'form-control', :disabled => true %>
	</div>
</div>

