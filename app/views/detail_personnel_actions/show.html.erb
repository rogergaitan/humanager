<%- model_class = DetailPersonnelAction -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:type_of_personnel_action_id) %>:</strong></dt>
  <dd><%= @detail_personnel_action.type_of_personnel_action_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fields_personnel_action_id) %>:</strong></dt>
  <dd><%= @detail_personnel_action.fields_personnel_action_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:value) %>:</strong></dt>
  <dd><%= @detail_personnel_action.value %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              detail_personnel_actions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_detail_personnel_action_path(@detail_personnel_action), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              detail_personnel_action_path(@detail_personnel_action),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
