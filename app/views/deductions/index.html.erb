<h1>Listing deductions</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Employee</th>
    <th>Frequency</th>
    <th>Calculation method</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @deductions.each do |deduction| %>
  <tr>
    <td><%= deduction.description %></td>
    <td><%= deduction.employee %></td>
    <td><%= deduction.frequency %></td>
    <td><%= deduction.calculation_method %></td>
    <td><%= link_to 'Show', deduction %></td>
    <td><%= link_to 'Edit', edit_deduction_path(deduction) %></td>
    <td><%= link_to 'Destroy', deduction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Deduction', new_deduction_path %>
