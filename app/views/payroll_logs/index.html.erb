<%- model_class = PayrollLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human(:count => 2) %></h1>
</div>
<%= will_paginate :class =>'pagination pagination-centered' %>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:payroll_id) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:task_id) %></th>
      <th><%= model_class.human_attribute_name(:time_worked) %></th>
      <th><%= model_class.human_attribute_name(:centro_de_costo_id) %></th>
      <th><%= model_class.human_attribute_name(:payment_type) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @payroll_logs.each do |payroll_log| %>
      <tr>
        <td><%= link_to payroll_log.id, payroll_log_path(payroll_log) %></td>
        <td><%= payroll_log.payroll_id %></td>
        <td><%= payroll_log.date %></td>
        <td><%= payroll_log.task_id %></td>
        <td><%= payroll_log.time_worked %></td>
        <td><%= payroll_log.centro_de_costo_id %></td>
        <td><%= payroll_log.payment_type %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_payroll_log_path(payroll_log), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      payroll_log_path(payroll_log),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_payroll_log_path,
            :class => 'btn btn-primary' %>
