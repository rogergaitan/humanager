<%- model_class = PayrollLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:payroll_id) %>:</strong></dt>
  <dd><%= @payroll_log.payroll_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:record_date) %>:</strong></dt>
  <dd><%= @payroll_log.record_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:task_id) %>:</strong></dt>
  <dd><%= @payroll_log.task_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:time_worked) %>:</strong></dt>
  <dd><%= @payroll_log.time_worked %></dd>
  <dt><strong><%= model_class.human_attribute_name(:centro_de_costo_id) %>:</strong></dt>
  <dd><%= @payroll_log.centro_de_costo_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:payment_type) %>:</strong></dt>
  <dd><%= @payroll_log.payment_type %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              payroll_logs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_payroll_log_path(@payroll_log), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              payroll_log_path(@payroll_log),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
